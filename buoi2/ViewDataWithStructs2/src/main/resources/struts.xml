<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="/" namespace="/" extends="struts-default">
        <action name="addProduct" class="com.fpt.controller.AddProduct" method="doAddProduct">
            <result name="ShowEdit">/jsp/ShowAddProduct.jsp</result>
        </action>
        <action name="viewProduct" class="com.fpt.controller.ViewProduct" method="getListProduct">
            <result name="view">/jsp/ShowProduct.jsp</result>
        </action>
    </package>
</struts>